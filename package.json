{
  "name": "qooqiephone",
  "productName": "Qooqie Phone",
  "version": "6.0.0",
  "private": true,
  "homepage": "./",
  "main": "electron/main.ts",
  "author": "Qooqie",
  "description": "Qooqie application",
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "Qooqie Phone",
        "icon": "./Icon/Icon",
        "name": "Qooqie Phone",
        "osxSign": {
          "identity": "Developer ID Application: Qooqie N.V. (DXR59ZN3G9)",
          "hardened-runtime": true,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist",
          "signature-flags": "library",
          "gatekeeper-assess": false
        },
        "osxNotarize": {
          "appleId": "richard@qooqie.com",
          "appleIdPassword": "xxrj-vixl-qtvx-ahza"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "QooqiePhone"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "./Icon/Icon"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "scripts": {
    "web": "react-scripts start",
    "start_": "craco start",
    "start": "electron-forge start",
    "rn": "react-native start",
    "android": "react-native run-android",
    "shake": "adb shell input keyevent 82",
    "ios": "react-native run-ios",
    "bitcode": "[ -d node_modules/react-native-webrtc/ios/WebRTC.dSYM ] || bash node_modules/react-native-webrtc/tools/downloadBitcode.sh",
    "clean": "yarn && rm -rf ios/build/* && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && pod install && cd ../android && ./gradlew clean",
    "build": "del-cli -f node_modules/.cache/babel* && craco build",
    "intl": "del-cli -f .babelPluginIntl.json && cross-env EXTRACT_INTL=1 yarn build",
    "format": "eslint --ignore-path=.gitignore --ext=.js,.ts,.tsx --fix . && prettier --loglevel=error --ignore-path=.gitignore --write .",
    "check-all": "yarn check-eslint && yarn check-prettier && yarn check-typescript && yarn check-type-coverage",
    "check-eslint": "eslint --ignore-path=.gitignore --ext=.js,.ts,.tsx --max-warnings=0 .",
    "check-prettier": "prettier --ignore-path=.gitignore --list-different .",
    "check-typescript": "tsc",
    "check-type-coverage": "type-coverage --ignoreCatch --atLeast=100",
    "postinstall": "husky install",
    "electron:start": "concurrently \"yarn start\" \"wait-on http://localhost:3001 && electron .\"",
    "elec-build": "PUBLIC_URL=/ react-scripts build",
    "build-electron": "react-scripts build",
    "electron:build": "yarn build && electron-builder build --publish never",
    "electron:build:all": "yarn build-electron && electron-builder build --publish never -mwl",
    "electron:run": "./node_modules/.bin/electron . --enable-logging --app-dir=/path/to/app",
    "build:electron": "env-cmd .env.electron craco",
    "react-build": "react-scripts build",
    "electron-build": "electron-builder",
    "test": "yarn react-build && yarn electron-build",
    "package": "electron-forge package",
    "start-forge": "electron-forge start",
    "forge-package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "npx @electron-forge/cli make --platform darwin",
    "make-linux": "npx @electron-forge/cli make --platform linux",
    "make1": "npx @electron-forge/cli make",
    "start1": "electron-forge start",
    "package1": "electron-forge package",
    "make-win": "electron-forge make  --asar=true",
    "make-win1": "--output-path=electron/dist/dist && electron-forge make"
  },
